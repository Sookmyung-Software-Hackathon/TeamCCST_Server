					"required": true,
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/UserCreateDto"
							}
						}
					}
				}
			}
		},
		"/auth/signin": {
			"post": {
				"operationId": "LoginUser",
				"responses": {
					"200": {
						"description": "Success",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/UserLoginResponseDto"
								}
							}
						}
					},
					"404": {
						"description": "Not Found - 이메일에 해당하는 사용자 정보 없음"
					},
					"409": {
						"description": "Invalid - 비밀번호가 올바르지 않음"
					}
				},
				"tags": [
					"Auth"
				],
				"security": [],
				"parameters": [],
